        "gulp-help": "^1.6.1",
        "gulp-msbuild": "^0.6.2",
        "gulp-npm-run": "0.0.3",
        "gulp-nunit-runner": "^1.2.3",
        "gulp-stream-to-promise": "^0.1.0",
        "gulp-util": "^3.0.8",
        "inquirer": "^7.1.0",
        "jasmine-core": "^3.5.0",
        "jsdom": "^16.2.2",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",

        "mocha": "^6.1.4",
        "mocha-yar": "^1.0.13",
        "nodemon": "^1.19.1",
        "npm-run-all": "^4.1.5",
        "plugin-error": "^1.0.1",
        "proxyquire": "^2.1.3",
        "q": "^1.5.1",
        "readline": "^1.3.0",
        "request": "^2.88.0",
        "require-dir": "^1.2.0",
        "requirejs": "^2.3.6",
        "rimraf": "^2.6.3",
        "run-sequence": "^2.2.1",
        "simple-git": "^1.124.0",

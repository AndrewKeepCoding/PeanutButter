        "del": "^5.1.0",
        "event-stream": "^4.0.1",
        "fancy-log": "^1.3.3",
        "gulp": "^4.0.2",
        "gulp-debug": "^4.0.0",
        "gulp-dotnet-cli": "^1.1.0",
        "gulp-edit-xml": "^3.1.1",
